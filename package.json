{
  "name": "nc_zoom",
  "version": "1.0.0",
  "description": "Nomad Coders - A Zoom clone made with WebRTC and SocketIO.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/librity/nc_zoom.git"
  },
  "keywords": [
    "zoom",
    "clone"
  ],
  "author": "@librity",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/librity/nc_zoom/issues"
  },
  "homepage": "https://github.com/librity/nc_zoom#readme",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier --write ./src",
    "build": "babel src --ignore src/public --out-dir build --copy-files",
