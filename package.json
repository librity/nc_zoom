    "prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@socket.io/admin-ui": "^0.2.0",
    "express": "^4.17.1",
    "localtunnel": "^2.0.2",
    "pug": "^3.0.2",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1"
  }
}
